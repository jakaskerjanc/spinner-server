{
  "name": "spinner-server",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "node dist/index.js",
    "build": "tsc",
    "dev": "nodemon --exec ts-node src/index.ts",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "type-check": "tsc --project tsconfig.json --pretty --noEmit",
    "scrapper:fetch": "node dist/scrapper/scrapper.js scrapeLatest",
    "scrapper:updateOnGoing": "node dist/scrapper/scrapper.js updateOnGoingDescriptions",
    "lint:prisma": "prisma format",
    "connect:main": "pscale connect spinner-db main --port 3309",
    "connect:dev": "pscale connect spinner-db dev --port 3309"
  },
  "dependencies": {
    "@prisma/client": "^4.15.0",
    "axios": "^1.3.4",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "lodash": "^4.17.21",
    "make-runnable": "^1.4.1",
    "rss-parser": "^3.12.0"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.17",
    "@types/lodash": "^4.14.192",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "eslint": "^8.0.1",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "nodemon": "^2.0.22",
    "prisma": "^4.15.0",
    "ts-node": "^10.9.1",
    "typescript": "4.6.4"
  }
}
